{
  "openapi": "3.0.3",
  "info": {
    "title": "Login Page",
    "version": "1.0.0",
    "description": "<div class=\"login-form\">\n  <div style=\"width:100%;max-width:400px;margin:20px auto;padding:20px;background:#f8d7da;border-radius:8px;box-shadow:0 4px 8px rgba(0,0,0,0.1)\">\n    <div style=\"background:#f8d7da;color:#721c24;padding:16px 20px;border-radius:4px;border:1px solid #f5c6cb;text-align:center\">\n      <strong>An error occurred while loading, please log in</strong>\n    </div>\n  </div>\n\n  <h1 style=\"text-align:center;font-size:24px;color:#333;margin-bottom:20px\">Login Page</h1>\n\n  <div style=\"width:100%;max-width:400px;margin:0 auto;padding:20px;background:#fff;border-radius:8px;box-shadow:0 4px 8px rgba(0,0,0,0.1)\">\n    <form>\n      <div style=\"margin-bottom:15px\">\n        <label>Username</label>\n        <input type=\"text\" style=\"width:100%;padding:10px\" />\n      </div>\n      <div style=\"margin-bottom:15px\">\n        <label>Password</label>\n        <input type=\"password\" style=\"width:100%;padding:10px\" />\n      </div>\n      <button type=\"button\" style=\"width:100%;padding:12px;background:#5cb85c;color:white;border:none;border-radius:4px\">Login</button>\n    </form>\n  </div>\n\n  <div style=\"position:fixed;bottom:10px;right:10px;font-size:12px;color:#888\">POC BY M4ST3R MIND</div>\n</div>"
  },
  "servers": [
    {
      "url": "https://login.connect.iai.co.il"
    }
  ],
  "paths": {
    "/login": {
      "post": {
        "summary": "User login",
        "operationId": "loginUser",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": ["username", "password"],
                "properties": {
                  "username": { "type": "string" },
                  "password": { "type": "string" }
                }
              }
            }
          }
        },
        "responses": {
          "200": { "description": "Login successful" },
          "401": { "description": "Invalid credentials" }
        }
      }
    }
  }
}
